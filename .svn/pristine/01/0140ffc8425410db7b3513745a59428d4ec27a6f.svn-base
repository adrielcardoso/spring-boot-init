package br.intelidata.entity;

public class ErrorMessage
{
	private String message;
	
	public ErrorMessage()
	{
		super();
	}
	
	public ErrorMessage(String message)
	{
		super();
		this.message=message;
	}
	
	public String getMessage()
	{
		return this.message;
	}
	
	public void setMessage(String message)
	{
		this.message=message;
	}
}
